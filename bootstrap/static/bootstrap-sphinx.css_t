/*
* bootstrap-sphinx.css
* ~~~~~~~~~~~~~~~~~~~~
*
* Sphinx stylesheet -- Twitter Bootstrap theme.
*/

.navbar-inverse .brand {
  color: #FFF;
}

{% if theme_navbar_fixed_top == 'true' %}
body {
  padding-top: 40px;
}
.navbar-inner {
  padding-left:  12px !important;
  padding-right: 12px !important;
}
{% endif %}

{%- if logo %}
.navbar .brand img {
  margin: none;
  padding: none;
  max-width: 24px;
  max-height: 24px;
}
{%- endif %}
